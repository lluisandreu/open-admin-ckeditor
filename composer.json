{
    "name": "lluisandreu/open-admin-ckeditor",
    "description": "Integrate CKEDITOR into open-admin",
    "type": "library",
    "keywords": [
        "open-admin",
        "wysiwyg",
        "extension",
        "editor",
        "CKEditor"
    ],
    "homepage": "https://github.com/lluisandreu/open-admin-ckeditor",
    "license": "MIT",
    "authors": [
        {
            "name": "open-admin",
            "email": "info@open-admin.org"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": ">=8.0",
        "open-admin-org/open-admin": "~1.0",
        "ckeditor/ckeditor": "^4.25.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "laravel/laravel": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Admin\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\Admin\\CKEditor\\CKEditorServiceProvider"
            ]
        }
    }
}